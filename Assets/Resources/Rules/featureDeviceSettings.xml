  <!-- DEVELOPMENT  -->
  <!-- =========================================================================================================================- -->
  <!-- 
      Banned settings are used to override the profile settings and they can be override by the particular settings for the device.
      Its purpose is to disable a feature that has been proven to be problematic on several devices. In this case the feature should be banned by default for all devices by adding adding it
      here and it should be enabled device by device (after testing it on the actual devices) on their corresponding entries in this same file      
   -->
  <Definition sku="banned"/>
  
  <!-- This entry is used to test different configuration when running on the editor -->
  <Definition sku="UNITY_EDITOR" profile="very_high"/>
  
  <!-- This entry is used to test that settings from server override the ones defined by content -->
  <Definition sku="server" entitiesLOD="high"/>

